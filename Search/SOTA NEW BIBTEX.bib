@article{rashed_one-year_2021,
	title = {One-{Year} {Lesson}: {Machine} {Learning} {Prediction} of {COVID}-19 {Positive} {Cases} with {Meteorological} {Data} and {Mobility} {Estimate} in {Japan}},
	volume = {18},
	issn = {1660-4601},
	shorttitle = {One-{Year} {Lesson}},
