@article{alamrouni_multi-regional_2022,
	title = {Multi-{Regional} {Modeling} of {Cumulative} {COVID}-19 {Cases} {Integrated} with {Environmental} {Forest} {Knowledge} {Estimation}: {A} {Deep} {Learning} {Ensemble} {Approach}},
